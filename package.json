    "test:webworker": "aegir test -t webworker",
    "lint": "aegir lint",
    "release": "aegir release",
    "release-minor": "aegir release --type minor",
    "release-major": "aegir release --type major",
    "coverage": "nyc -s npm run test:node && nyc report --reporter=html",
